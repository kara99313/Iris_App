</span><span><span class="hljs-keyword">with</span></span><span> col1:
    st.image(</span><span><span class="hljs-string">"https://upload.wikimedia.org/wikipedia/commons/4/41/Iris_versicolor_3.jpg"</span></span><span>, width=</span><span><span class="hljs-number">100</span></span><span>)
</span><span><span class="hljs-keyword">with</span></span><span> col2:
    st.markdown(</span><span><span class="hljs-string">"## 🌸 Application de Prédiction Professionnelle de Fleurs d'Iris"</span></span><span>)
    st.markdown(</span><span><span class="hljs-string">"Utilisez les paramètres ci-dessous pour prédire l’espèce d’une fleur et explorer les résultats visuellement."</span></span><span>)

</span><span><span class="hljs-comment"># --- Sidebar ---</span></span><span>
st.sidebar.header(</span><span><span class="hljs-string">"🔧 Paramètres de la fleur"</span></span><span>)

sepal_length = st.sidebar.slider(</span><span><span class="hljs-string">"Longueur du sépale (cm)"</span></span><span>, </span><span><span class="hljs-number">4.0</span></span><span>, </span><span><span class="hljs-number">8.0</span></span><span>, </span><span><span class="hljs-number">5.1</span></span><span>)
