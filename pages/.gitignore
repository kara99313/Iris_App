data = pd.DataFrame([[</span><span><span class="hljs-number">5.1</span></span><span>, </span><span><span class="hljs-number">3.5</span></span><span>, </span><span><span class="hljs-number">1.4</span></span><span>, </span><span><span class="hljs-number">0.2</span></span><span>]], columns=[</span><span><span class="hljs-string">'sepal length'</span></span><span>, </span><span><span class="hljs-string">'sepal width'</span></span><span>, </span><span><span class="hljs-string">'petal length'</span></span><span>, </span><span><span class="hljs-string">'petal width'</span></span><span>])

</span><span><span class="hljs-comment"># Extraire uniquement les valeurs sans les noms de colonnes</span></span><span>
