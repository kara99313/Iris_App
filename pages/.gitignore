        st.markdown(</span><span><span class="hljs-string">f"&lt;style&gt;<span class="hljs-subst">{f.read()}</span></span></span><span>&lt;/style&gt;", unsafe_allow_html=</span><span><span class="hljs-literal">True</span></span><span>)

load_local_css(</span><span><span class="hljs-string">"assets/style.css"</span></span><span>)

</span><span><span class="hljs-comment"># 🧼 Masquer les éléments par défaut de Streamlit</span></span><span>
st.markdown(<span class="hljs-string">"""
    &lt;style&gt;
        #MainMenu {visibility: hidden;}
        footer {visibility: hidden;}
    &lt;/style&gt;
"""</span>, unsafe_allow_html=</span><span><span class="hljs-literal">True</span></span><span>)

</span><span><span class="hljs-comment"># 👑 Titre principal</span></span><span>
st.title(</span><span><span class="hljs-string">"🌸 Application de Prédiction de Fleurs Iris"</span></span><span>)

</span><span><span class="hljs-comment"># 🧭 Création de la barre de navigation</span></span><span>
PAGES = {
    </span><span><span class="hljs-string">"Accueil"</span></span><span>: </span><span><span class="hljs-string">"pages/1_🏠_Accueil.py"</span></span><span>,
    </span><span><span class="hljs-string">"Prédiction"</span></span><span>: </span><span><span class="hljs-string">"pages/2_🔍_Prédiction.py"</span></span><span>,
    </span><span><span class="hljs-string">"Historique"</span></span><span>: </span><span><span class="hljs-string">"pages/3_📊_Historique.py"</span></span><span>,
    </span><span><span class="hljs-string">"Analyse"</span></span><span>: </span><span><span class="hljs-string">"pages/4_📈_Analyse.py"</span></span><span>,
    </span><span><span class="hljs-string">"Paramètres"</span></span><span>: </span><span><span class="hljs-string">"pages/5_⚙️_Paramètres.py"</span></span><span>,
    </span><span><span class="hljs-string">"À propos"</span></span><span>: </span><span><span class="hljs-string">"pages/6_ℹ️_À_propos.py"</span></span><span>,
}

</span><span><span class="hljs-comment"># Navigation par menu radio</span></span><span>
page = st.sidebar.radio(</span><span><span class="hljs-string">"Naviguer"</span></span><span>, </span><span><span class="hljs-built_in">list</span></span><span>(PAGES.keys()))

</span><span><span class="hljs-comment"># 🚨 Correction ici : pour afficher dynamiquement le contenu de chaque page,</span></span><span>
</span><span><span class="hljs-comment"># utilise `exec` au lieu de `st.markdown(f.read())`</span></span><span>
</span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(PAGES[page], encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
    code = f.read()
    </span><span><span class="hljs-built_in">exec</span></span><span>(code)
</span></span></code></div></div></pre>
