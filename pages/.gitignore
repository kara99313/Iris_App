petal_width = st.sidebar.slider(</span><span><span class="hljs-string">"Largeur du pétale (cm)"</span></span><span>, </span><span><span class="hljs-number">0.1</span></span><span>, </span><span><span class="hljs-number">2.5</span></span><span>, </span><span><span class="hljs-number">0.2</span></span><span>)

</span><span><span class="hljs-comment"># --- Chargement modèle ---</span></span><span>
model = joblib.load(</span><span><span class="hljs-string">"iris_model.pkl"</span></span><span>)
species = [</span><span><span class="hljs-string">'Setosa'</span></span><span>, </span><span><span class="hljs-string">'Versicolor'</span></span><span>, </span><span><span class="hljs-string">'Virginica'</span></span><span>]

</span><span><span class="hljs-comment"># --- Prédiction ---</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> st.sidebar.button(</span><span><span class="hljs-string">"🔍 Prédire l'espèce"</span></span><span>):

    input_data = pd.DataFrame([[sepal_length, sepal_width, petal_length, petal_width]],
        columns=[</span><span><span class="hljs-string">'sepal length (cm)'</span></span><span>, </span><span><span class="hljs-string">'sepal width (cm)'</span></span><span>, </span><span><span class="hljs-string">'petal length (cm)'</span></span><span>, </span><span><span class="hljs-string">'petal width (cm)'</span></span><span>])

    prediction = model.predict(input_data)[</span><span><span class="hljs-number">0</span></span><span>]
    probas = model.predict_proba(input_data)[</span><span><span class="hljs-number">0</span></span><span>]
    predicted_species = species[prediction]

    st.success(</span><span><span class="hljs-string">f"🌿 Espèce prédite : **<span class="hljs-subst">{predicted_species}</span></span></span><span>**")

    </span><span><span class="hljs-comment"># --- Probabilités ---</span></span><span>
    st.subheader(</span><span><span class="hljs-string">"📊 Probabilités de Prédiction"</span></span><span>)
    fig_prob = go.Figure(go.Bar(
        x=species,
        y=probas,
        marker_color=[</span><span><span class="hljs-string">'#2a9d8f'</span></span><span>, </span><span><span class="hljs-string">'#f4a261'</span></span><span>, </span><span><span class="hljs-string">'#e76f51'</span></span><span>]
    ))
    fig_prob.update_layout(title=</span><span><span class="hljs-string">"Probabilité d'appartenance à chaque espèce"</span></span><span>, yaxis=</span><span><span class="hljs-built_in">dict</span></span><span>(</span><span><span class="hljs-built_in">range</span></span><span>=[</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">1</span></span><span>]))
    st.plotly_chart(fig_prob, use_container_width=</span><span><span class="hljs-literal">True</span></span><span>)

    </span><span><span class="hljs-comment"># --- Enregistrement CSV ---</span></span><span>
    result = {
        </span><span><span class="hljs-string">"Timestamp"</span></span><span>: datetime.now().strftime(</span><span><span class="hljs-string">"%Y-%m-%d %H:%M:%S"</span></span><span>),
        </span><span><span class="hljs-string">"Sepal Length"</span></span><span>: sepal_length,
        </span><span><span class="hljs-string">"Sepal Width"</span></span><span>: sepal_width,
        </span><span><span class="hljs-string">"Petal Length"</span></span><span>: petal_length,
        </span><span><span class="hljs-string">"Petal Width"</span></span><span>: petal_width,
        </span><span><span class="hljs-string">"Prediction"</span></span><span>: predicted_species,
        </span><span><span class="hljs-string">"Prob_Setosa"</span></span><span>: </span><span><span class="hljs-built_in">round</span></span><span>(probas[</span><span><span class="hljs-number">0</span></span><span>], </span><span><span class="hljs-number">3</span></span><span>),
        </span><span><span class="hljs-string">"Prob_Versicolor"</span></span><span>: </span><span><span class="hljs-built_in">round</span></span><span>(probas[</span><span><span class="hljs-number">1</span></span><span>], </span><span><span class="hljs-number">3</span></span><span>),
        </span><span><span class="hljs-string">"Prob_Virginica"</span></span><span>: </span><span><span class="hljs-built_in">round</span></span><span>(probas[</span><span><span class="hljs-number">2</span></span><span>], </span><span><span class="hljs-number">3</span></span><span>)
    }

    result_df = pd.DataFrame([result])
    file_exists = os.path.exists(</span><span><span class="hljs-string">"predictions.csv"</span></span><span>)

    </span><span><span class="hljs-keyword">if</span></span><span> file_exists:
        result_df.to_csv(</span><span><span class="hljs-string">"predictions.csv"</span></span><span>, mode=</span><span><span class="hljs-string">'a'</span></span><span>, header=</span><span><span class="hljs-literal">False</span></span><span>, index=</span><span><span class="hljs-literal">False</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        result_df.to_csv(</span><span><span class="hljs-string">"predictions.csv"</span></span><span>, index=</span><span><span class="hljs-literal">False</span></span><span>)

    st.info(</span><span><span class="hljs-string">"📁 Prédiction enregistrée dans le fichier `predictions.csv`."</span></span><span>)

    </span><span><span class="hljs-comment"># --- Visualisation des caractéristiques ---</span></span><span>
    st.subheader(</span><span><span class="hljs-string">"📈 Caractéristiques de la fleur"</span></span><span>)
    st.bar_chart(pd.DataFrame({
        </span><span><span class="hljs-string">"Valeur (cm)"</span></span><span>: [sepal_length, sepal_width, petal_length, petal_width]
    }, index=[</span><span><span class="hljs-string">"Longueur Sépale"</span></span><span>, </span><span><span class="hljs-string">"Largeur Sépale"</span></span><span>, </span><span><span class="hljs-string">"Longueur Pétale"</span></span><span>, </span><span><span class="hljs-string">"Largeur Pétale"</span></span><span>]))
</span></span></code></div></div></pre>
