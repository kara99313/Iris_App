# --- Charger le modèle ---
model = joblib.load("iris_model.pkl")
species = ['Setosa', 'Versicolor', 'Virginica']

# --- Barre latérale pour les entrées utilisateur ---
st.sidebar.header("🧪 Paramètres de la fleur")

sepal_length = st.sidebar.slider("Longueur du sépale (cm)", 4.0, 8.0, 5.1)
sepal_width = st.sidebar.slider("Largeur du sépale (cm)", 2.0, 4.5, 3.5)
petal_length = st.sidebar.slider("Longueur du pétale (cm)", 1.0, 7.0, 1.4)
petal_width = st.sidebar.slider("Largeur du pétale (cm)", 0.1, 2.5, 0.2)

# --- Bouton pour prédire ---
if st.sidebar.button("🔍 Prédire l'espèce"):

    input_data = pd.DataFrame([[sepal_length, sepal_width, petal_length, petal_width]],
                              columns=['sepal length (cm)', 'sepal width (cm)',
                                       'petal length (cm)', 'petal width (cm)'])

    prediction = model.predict(input_data)
    predicted_species = species[prediction[0]]

    # --- Affichage du résultat ---
    st.markdown(f"&lt;h2 style='text-align: center; color: #4CAF50;'&gt;🌿 Résultat : {predicted_species}&lt;/h2&gt;", unsafe_allow_html=True)

    # --- Visualisation des paramètres ---
    st.subheader("🔬 Profil de la fleur")
    fig, ax = plt.subplots()
    features = ['Sepal Length', 'Sepal Width', 'Petal Length', 'Petal Width']
    values = [sepal_length, sepal_width, petal_length, petal_width]
    bars = ax.bar(features, values, color=['#f4a261', '#2a9d8f', '#e76f51', '#264653'])
    ax.set_ylim(0, 8)
    ax.set_ylabel("cm")
    st.pyplot(fig)

