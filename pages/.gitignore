sepal_length = st.sidebar.slider(</span><span><span class="hljs-string">"Longueur du sépale (cm)"</span></span><span>, </span><span><span class="hljs-number">4.0</span></span><span>, </span><span><span class="hljs-number">8.0</span></span><span>, </span><span><span class="hljs-number">5.1</span></span><span>)
sepal_width = st.sidebar.slider(</span><span><span class="hljs-string">"Largeur du sépale (cm)"</span></span><span>, </span><span><span class="hljs-number">2.0</span></span><span>, </span><span><span class="hljs-number">4.5</span></span><span>, </span><span><span class="hljs-number">3.5</span></span><span>)
petal_length = st.sidebar.slider(</span><span><span class="hljs-string">"Longueur du pétale (cm)"</span></span><span>, </span><span><span class="hljs-number">1.0</span></span><span>, </span><span><span class="hljs-number">7.0</span></span><span>, </span><span><span class="hljs-number">1.4</span></span><span>)
petal_width = st.sidebar.slider(</span><span><span class="hljs-string">"Largeur du pétale (cm)"</span></span><span>, </span><span><span class="hljs-number">0.1</span></span><span>, </span><span><span class="hljs-number">2.5</span></span><span>, </span><span><span class="hljs-number">0.2</span></span><span>)

</span><span><span class="hljs-comment"># --- Bouton pour prédire ---</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> st.sidebar.button(</span><span><span class="hljs-string">"🔍 Prédire l'espèce"</span></span><span>):

    input_data = pd.DataFrame([[sepal_length, sepal_width, petal_length, petal_width]],
                              columns=[</span><span><span class="hljs-string">'sepal length (cm)'</span></span><span>, </span><span><span class="hljs-string">'sepal width (cm)'</span></span><span>,
                                       </span><span><span class="hljs-string">'petal length (cm)'</span></span><span>, </span><span><span class="hljs-string">'petal width (cm)'</span></span><span>])

    prediction = model.predict(input_data)
    predicted_species = species[prediction[</span><span><span class="hljs-number">0</span></span><span>]]

    </span><span><span class="hljs-comment"># --- Affichage du résultat ---</span></span><span>
    st.markdown(</span><span><span class="hljs-string">f"&lt;h2 style='text-align: center; color: #4CAF50;'&gt;🌿 Résultat : <span class="hljs-subst">{predicted_species}</span></span></span><span>&lt;/h2&gt;", unsafe_allow_html=</span><span><span class="hljs-literal">True</span></span><span>)

    </span><span><span class="hljs-comment"># --- Visualisation des paramètres ---</span></span><span>
    st.subheader(</span><span><span class="hljs-string">"🔬 Profil de la fleur"</span></span><span>)
    fig, ax = plt.subplots()
    features = [</span><span><span class="hljs-string">'Sepal Length'</span></span><span>, </span><span><span class="hljs-string">'Sepal Width'</span></span><span>, </span><span><span class="hljs-string">'Petal Length'</span></span><span>, </span><span><span class="hljs-string">'Petal Width'</span></span><span>]
    values = [sepal_length, sepal_width, petal_length, petal_width]
    bars = ax.bar(features, values, color=[</span><span><span class="hljs-string">'#f4a261'</span></span><span>, </span><span><span class="hljs-string">'#2a9d8f'</span></span><span>, </span><span><span class="hljs-string">'#e76f51'</span></span><span>, </span><span><span class="hljs-string">'#264653'</span></span><span>])
    ax.set_ylim(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">8</span></span><span>)
